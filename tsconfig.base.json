{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "Bundler",
  
      "strict": true,
      "strictNullChecks": false,
      "noImplicitAny": true,
      "noImplicitOverride": true,
      "noUncheckedIndexedAccess": true,
  
      "declaration": true,
      "declarationMap": true,
      "sourceMap": true,
      

  
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
  
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
  
      "baseUrl": "."
    }
  }
  